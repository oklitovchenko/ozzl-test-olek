#!/bin/sh -e

case "$1" in
    install|upgrade)
        getent passwd ozzl_test > /dev/null || \
            adduser --system --group --no-create-home ozzl_test
        adduser ozzl_test ozzl_test
        ;;
    abort-upgrade)
        ;;
    *)
        echo "preinst called with unknown argument '$1'" >&2
        exit 1
        ;;
esac

# dh_installdeb will replace this with shell code automatically
# generated by other debhelper scripts.

#DEBHELPER#

exit 0
